{
  "name": "jcatalog-react-i18n",
  "version": "1.1.0",
  "description": "jCatalog React I18n Extension",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf esdoc",
    "lint": "eslint . --ext .js,.jsx",
    "jscs": "jscs .",
    "test": "cross-env NODE_ENV=test mocha src/**/*.spec.js",
    "start": "cross-env NODE_ENV=development node src/www/devServer.js",
    "esdoc": "esdoc -c esdoc.json",
    "build-npm": "babel --ignore *.spec.js src --out-dir lib",
    "publish-test": "rimraf lib && npm run build-npm && fluid-publish --options=\"{\\\"devTag\\\": \\\"SNAPSHOT\\\"}\" --test",
    "publish-dev": "rimraf lib && npm run build-npm && fluid-publish --options=\"{\\\"devTag\\\": \\\"SNAPSHOT\\\"}\"",
    "publish-release": "rimraf lib && npm run build-npm && fluid-publish --standard"
  },
  "author": {
    "name": "Alexander Frolov",
    "email": "alexander.frolov@jcatalog.com"
  },
  "license": "UNLICENSED",
  "readme": "README.md",
  "dependencies": {
    "babel-eslint": "5.0.0",
    "intl": "1.1.0",
    "jcatalog-i18n": "1.1.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "6.4.0",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-react-hmre": "1.1.0",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.5.2",
    "chai": "3.5.0",
    "cross-env": "1.0.7",
    "css-loader": "0.21.0",
    "esdoc": "0.4.3",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "3.1.0",
    "eslint-plugin-react": "3.16.1",
    "express": "4.13.4",
    "jscs": "2.9.0",
    "jsdom": "8.0.2",
    "json-loader": "0.5.4",
    "mocha": "2.4.5",
    "react-addons-test-utils": "0.14.6",
    "rimraf": "2.5.2",
    "style-loader": "0.13.0",
    "webpack": "1.12.2",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.7.1",
    "fluid-publish": "^1.1.0"
  },
  "engines": {
    "node": "4.x",
    "npm": "2.1.x"
  }
}
