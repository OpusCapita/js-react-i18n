{
  "name": "@opuscapita/react-i18n",
  "version": "1.0.2",
  "description": "OpusCapita React I18n Extension",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf esdoc build lib",
    "lint": "eslint src",
    "test": "mocha src/**/*.spec.js --require test/mocha-setup.js",
    "start": "cross-env NODE_ENV=development node src/www/devServer.js",
    "esdoc": "rimraf esdoc && esdoc -c esdoc.json",

    "npm-build": "rimraf lib && babel --copy-files --no-babelrc --presets es2015,stage-0,react --ignore *.spec.js src/app --out-dir lib",
    "npm-publish": "npm run npm-build && npm-publish",

    "publish-snapshot": "npm run npm-publish",
    "publish-release": "npm run npm-publish -- --release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapitaBES/js-react-i18n.git"
  },
  "author": {
    "name": "Alexander Frolov",
    "email": "alexander.frolov@jcatalog.com"
  },
  "license": "UNLICENSED",
  "readme": "README.md",
  "dependencies": {
    "@opuscapita/i18n": "1.0.3",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-core": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "cross-env": "1.0.7",
    "esdoc": "0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "2.10.2",
    "eslint-config-jcatalog": "1.0.1",
    "eslint-plugin-react": "5.1.1",
    "express": "4.13.4",
    "jsdom": "8.0.2",
    "json-loader": "0.5.4",
    "mocha": "2.4.5",
    "react-addons-test-utils": "0.14.6",
    "rimraf": "2.5.2",
    "webpack": "1.12.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "jcatalog-dev-tools": "2.0.14"
  },
  "engines": {
    "node": ">=4.x",
    "npm": ">=2.x"
  }
}
