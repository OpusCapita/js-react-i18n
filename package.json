{
  "name": "jcatalog-react-i18n",
  "version": "8.0.0",
  "description": "jCatalog React I18n Extension",
  "main": "index.js",
  "private": "true",
  "files": [
    "dist",
    "lib",
    "scripts/fix-prepublish.js",
    "src",
    ".babelrc",
    "index.js"
  ],
  "scripts": {
    "clean": "rimraf build/*.js dist esdoc lib",
    "lint": "eslint src",
    "jscs": "jscs src",
    "test": "mocha src/**/*.spec.js",
    "build": "npm run clean && npm run lint && npm run jscs && npm test && npm run build-lib && npm run build-umd && npm run build-umd-min",
    "build-lib": "babel src --out-dir lib",
    "build-umd": "cross-env NODE_ENV=development webpack --progress --colors --config webpack.config.js",
    "build-umd-min": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.js",
    "start": "cross-env NODE_ENV=production webpack-dev-server",
    "esdoc": "esdoc -c esdoc.json",
    "postinstall": "node ./scripts/fix-prepublish.js postinstall",
    "prepublish": "npm run build-lib"
  },
  "author": {
    "name": "Alexander Frolov",
    "email": "alexander.frolov@jcatalog.com"
  },
  "license": "UNLICENSED",
  "readme": "README.md",
  "dependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "intl": "^1.0.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.7",
    "jcatalog-i18n": "git://192.168.7.59/#webpack",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "chai": "^3.4.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.21.0",
    "esdoc": "^0.4.3",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.16.1",
    "html-webpack-plugin": "^2.8.1",
    "jscs": "^2.9.0",
    "jsdom": "^8.0.2",
    "json-loader": "^0.5.4",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.7.3"
  },
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.6.0"
  }
}
