{
  "name": "jcatalog-react-i18n",
  "version": "1.1.1",
  "description": "jCatalog React I18n Extension",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf esdoc build lib",
    "lint": "eslint src",
    "test": "mocha src/**/*.spec.js --require test/mocha-setup.js",
    "start": "cross-env NODE_ENV=development node src/www/devServer.js",
    "esdoc": "rimraf esdoc && esdoc -c esdoc.json",
    "build-npm": "rimraf lib && babel --copy-files --no-babelrc --presets es2015,stage-0,react --ignore *.spec.js src --out-dir lib",
    "publish-test": "npm run build-npm && fluid-publish --options=\"{\\\"devTag\\\": \\\"SNAPSHOT\\\"}\" --test",
    "publish-dev": "npm run build-npm && fluid-publish --options=\"{\\\"devTag\\\": \\\"SNAPSHOT\\\"}\"",
    "publish-release": "npm run build-npm && fluid-publish --standard"
  },
  "repository": {
    "type": "git",
    "url": "http://buildserver.jcatalog.com/git/js-react-i18n.git"
  },
  "author": {
    "name": "Alexander Frolov",
    "email": "alexander.frolov@jcatalog.com"
  },
  "license": "UNLICENSED",
  "readme": "README.md",
  "dependencies": {
    "jcatalog-i18n": "1.1.0",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-core": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "cross-env": "1.0.7",
    "esdoc": "0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "2.10.2",
    "eslint-config-jcatalog": "1.0.1",
    "eslint-plugin-react": "5.1.1",
    "express": "4.13.4",
    "jsdom": "8.0.2",
    "json-loader": "0.5.4",
    "mocha": "2.4.5",
    "react-addons-test-utils": "0.14.6",
    "rimraf": "2.5.2",
    "webpack": "1.12.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "fluid-publish": "1.1.0"
  },
  "engines": {
    "node": "4.x",
    "npm": "2.x"
  }
}
